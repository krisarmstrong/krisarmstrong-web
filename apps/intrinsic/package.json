{
  "name": "intrinsicmomentummindset-com",
  "author": "Kris Armstrong",
  "private": true,
  "version": "0.3.1",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run sitemap && vite build",
    "lint": "eslint . --max-warnings=0 && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest --pool=threads",
    "test:ui": "vitest --ui --pool=threads",
    "test:coverage": "vitest --coverage --pool=threads",
    "sitemap": "node scripts/generate-sitemap.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@krisarmstrong/web-foundation": "*",
    "@sentry/react": "^10.25.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "4.0.9",
    "@vitest/coverage-v8": "4.0.9",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  }
}
